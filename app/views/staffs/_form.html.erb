<%= form_for(@staff) do |f| %>
<div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
</div>
    
<div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :email %>
</div>
    
<div class="field">
    <%= f.label :birthday %><br>
    <%= f.date_select :birthday,:start_year=>1900 %>
</div>
    
<div class="field">
    <%= f.label :job %><br>
    <%= f.text_field :job %>
</div>
    
<div class="field">
    <%= f.label :region %><br>
    <%= f.text_field :region %>
</div>
    
<div class="field">
    <%= f.label :memo %><br>
    <%= f.text_area:memo%>
</div>

<div class="field">
    <div class="action ">
    <%= f.submit %>
    </div>
</div>
<% end %>